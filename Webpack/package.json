{
	"name": "webpack",
	"version": "1.0.0",
	"description": "",
	"main": "index.tsx",
	"scripts": {
		"start": "run-p -l type-check:watch start:dev",
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"start:dev": "webpack-dev-server --mode development --open --config webpack.dev.js",
		"start:prod": "webpack-dev-server --mode production --open --config webpack.prod.js",
		"build:dev": "rimraf dist && webpack --config webpack.dev.js",
		"build:prod": "rimraf dist && webpack --config webpack.prod.js",
		"build:perf": "rimraf dist && webpack --config webpack.perf.js"
	},
	"author": "Sergio Linero Aguilar",
	"license": "ISC",
	"dependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/preset-env": "^7.9.0",
		"@babel/preset-react": "^7.9.4",
		"@babel/preset-typescript": "^7.9.0",
		"@types/react": "^16.9.26",
		"@types/react-dom": "^16.9.5",
		"babel-loader": "^8.1.0",
		"css-loader": "^3.4.2",
		"dotenv-webpack": "^1.7.0",
		"file-loader": "^6.0.0",
		"html-loader": "^1.0.0",
		"html-webpack-plugin": "^4.0.1",
		"mini-css-extract-plugin": "^0.9.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"sass": "^1.26.3",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.1.3",
		"typescript": "^3.8.3",
		"url-loader": "^4.0.0",
		"webpack": "^4.42.1",
		"webpack-bundle-analyzer": "^3.6.1",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3",
		"webpack-merge": "^4.2.2"
	},
	"devDependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	}
}
